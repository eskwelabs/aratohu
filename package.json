{
  "name": "aratohu",
  "version": "1.0.0",
  "private": true,
  "description": "Video guide enhancement to Table of Contents extension",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/eskwelabs/aratohu",
  "bugs": {
    "url": "https://github.com/eskwelabs/aratohu/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eskwelabs/aratohu.git"
  },
  "license": "BSD-3-Clause",
  "author": "Eskwelabs",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "schema/*.json",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^3.1.1",
    "@jupyterlab/apputils": "^3.1.1",
    "@jupyterlab/cells": "^3.1.1",
    "@jupyterlab/coreutils": "^5.1.1",
    "@jupyterlab/docmanager": "^3.1.1",
    "@jupyterlab/docregistry": "^3.1.1",
    "@jupyterlab/fileeditor": "^3.1.1",
    "@jupyterlab/markdownviewer": "^3.1.1",
    "@jupyterlab/notebook": "^3.1.1",
    "@jupyterlab/rendermime": "^3.1.1",
    "@lumino/algorithm": "^1.6.0",
    "@lumino/coreutils": "^1.8.0",
    "@lumino/messaging": "^1.7.0",
    "@lumino/widgets": "^1.26.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.1",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "resolutions": {
    "@types/react": "~16.4.13"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": "lib/extension.js",
    "schemaDir": "schema"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
